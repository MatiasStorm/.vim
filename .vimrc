set nocompatible

# Color scheme:
g:gruvbox_italic=1
set termguicolors
autocmd vimenter * colorscheme gruvbox

set backspace=indent,eol,start
set ruler
set number
set showcmd
set incsearch
set hlsearch
set nrformats=
set tabstop=8 softtabstop=0 expandtab shiftwidth=4 smarttab " 4 spaces instead of tab
set clipboard=unnamedplus
map K gt
map J gT

syntax on

