{
  "diagnostic.checkCurrentLine": true,
  "coc.preferences.formatOnSaveFiletypes": ["elm"],
  "languageserver": {
    "elmLS": {
      "command": "elm-language-server",
      "filetypes": ["elm"],
      "rootPatterns": ["elm.json"],
      "initializationOptions": {
        "elmAnalyseTrigger": "never"
      }
    }
  },
  "codeLens.enable": true
}
